#! /bin/bash

# blastp -query  -db  -evalue 1e-5 -outfmt "6 qseqid staxids sseqid sgi sacc evalue" -out  -max_target_seqs 10 -max_hsps 1 -num_threads 1  -out


blastp -max_target_seqs 10000 -max_hsps 1 -num_threads 15 -evalue 1e-3 -outfmt "6 qseqid staxids sseqid sgi sacc evalue" -query species19/Metazoa-Annelida-Polychaeta-Order_unclassified14-Capitellidae-Capitella-teleta-283909-T1.aa -db species19/Metazoa-Annelida-Polychaeta-Order_unclassified14-Capitellidae-Capitella-teleta-283909-T1.aa  -out blast-1e-3/Capitella-teleta &&
